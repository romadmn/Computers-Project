services:
  computers.angular.app:
    build:
      context: C:\Users\rfere\source\repos\ComputersApp
      dockerfile: ComputersApp.Api/ClientApp/Dockerfile
    image: computersangularapp
    ports:
    - 4200:4200/tcp
    volumes:
    - C:\Users\rfere\source\repos\ComputersApp\ComputersApp.Api\ClientApp:/app:rw
  computersapp:
    build:
      args:
        skip_client_build: "true"
      context: C:\Users\rfere\source\repos\ComputersApp
      dockerfile: ComputersApp.Api/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: ComputersApp.Api
      target: base
    container_name: ComputersApp.Api
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: computersapiapp:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/netcoreapp3.1/ComputersApp.Api.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - 80/tcp
    - 443/tcp
    tty: true
    volumes:
    - C:\Users\rfere\source\repos\ComputersApp\ComputersApp.Api:/app:rw
    - C:\Users\rfere\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\rfere\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\rfere\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\rfere\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\rfere\source\repos\ComputersApp:/src:rw
  ms-sql-server:
    environment:
      ACCEPT_EULA: "Y"
      MSSQL_PID: Express
      SA_PASSWORD: MyPassword111
    image: mcr.microsoft.com/mssql/server:2019-latest
    ports:
    - 1433:1433/tcp
version: '3.4'